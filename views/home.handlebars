          <h5 class="card-title">{{this.title}}</h5>
          <p class="card-text">{{this.summary}}</p>
          <a href="{{this.link}}" class="card-link">Link to Article</a>
        </div>
      </div>
    </div>
    {{!-- not sending it to card, going below it --}}
    <div id="notes"></div>
  </div>

